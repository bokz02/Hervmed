<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/main2"
    android:background="@color/blueBackground">



    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guide1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".385"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guide2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".90"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guide3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".075"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guide4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".925"/>

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="220dp"
        android:layout_height="140dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/hervmed2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="120dp"
        app:layout_constraintBottom_toTopOf="@+id/cardView2"
        app:layout_constraintEnd_toEndOf="@id/guide4"
        app:layout_constraintStart_toStartOf="@id/guide3"
        android:layout_marginBottom="15dp">

        <androidx.cardview.widget.CardView
            android:id="@+id/cardView"
            android:layout_width="0dp"
            android:layout_height="70dp"
            app:cardCornerRadius="10dp"
            app:cardElevation="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent">

            <TextView
                android:id="@+id/textViewDidYouKnow"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="10dp"
                android:gravity="center"
                android:text="Text"
                android:textSize="14sp"/>

        </androidx.cardview.widget.CardView>


        <ImageView
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:background="@drawable/bulb2"
            android:translationZ="91dp"
            android:layout_marginTop="5dp"
            android:layout_marginStart="85dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/didyouknow"
            android:layout_width="100dp"
            android:layout_height="25dp"
            android:background="@color/greenPastel"
            android:translationZ="90dp"
            android:layout_marginTop="30dp"
            android:layout_marginStart="10dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">


            <TextView
                android:id="@+id/textView7"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="Did you know?"
                android:textSize="14sp"
                android:textStyle="bold|italic"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


        </androidx.constraintlayout.widget.ConstraintLayout>




    </androidx.constraintlayout.widget.ConstraintLayout>



    <androidx.cardview.widget.CardView
        android:id="@+id/cardView2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:cardCornerRadius="10dp"
        app:cardElevation="10dp"
        app:layout_constraintBottom_toBottomOf="@id/guide2"
        app:layout_constraintLeft_toLeftOf="@id/guide3"
        app:layout_constraintRight_toRightOf="@id/guide4"
        app:layout_constraintTop_toTopOf="@id/guide1">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/whiteParent"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageButton
                android:id="@+id/leaf2"
                android:layout_width="85dp"
                android:layout_height="85dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/leaf1"
                app:layout_constraintEnd_toStartOf="@+id/leaf3"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintStart_toEndOf="@+id/heart1"
                app:layout_constraintTop_toTopOf="parent">

            </ImageButton>

            <ImageButton
                android:id="@+id/heart1"
                android:layout_width="85dp"
                android:layout_height="85dp"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:background="@drawable/heart"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

            </ImageButton>

            <ImageButton
                android:id="@+id/leaf3"
                android:layout_width="85dp"
                android:layout_height="85dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="20dp"
                android:background="@drawable/leaf2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageButton
                android:id="@+id/drug"
                android:layout_width="85dp"
                android:layout_height="85dp"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:background="@drawable/bottle1"
                android:layout_marginTop="15dp"
                app:layout_constraintEnd_toEndOf="@+id/leaf2"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintStart_toStartOf="@+id/leaf2"
                app:layout_constraintTop_toBottomOf="@+id/textView2" />

            <ImageButton
                android:id="@+id/leaf4"
                android:layout_width="85dp"
                android:layout_height="85dp"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:background="@drawable/leaf4"
                android:layout_marginTop="15dp"
                app:layout_constraintEnd_toEndOf="@+id/heart1"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toLeftOf="@+id/drug"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintStart_toStartOf="@+id/heart1"
                app:layout_constraintTop_toBottomOf="@+id/textView" />

            <ImageButton
                android:id="@+id/poison"
                android:layout_width="85dp"
                android:layout_height="85dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/poison1"
                app:layout_constraintEnd_toEndOf="@+id/leaf3"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintLeft_toRightOf="@+id/drug"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintStart_toStartOf="@+id/leaf3"
                app:layout_constraintTop_toBottomOf="@+id/textView3" />

            <ImageButton
                android:id="@+id/edible"
                android:layout_width="85dp"
                android:layout_height="85dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/spoon3"
                app:layout_constraintEnd_toEndOf="@+id/leaf4"
                app:layout_constraintStart_toStartOf="@+id/leaf4"
                app:layout_constraintTop_toBottomOf="@+id/textView4" />

            <TextView
                android:id="@+id/textView"
                android:layout_width="50dp"
                android:layout_height="20dp"
                android:text="Popular"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="@+id/heart1"
                app:layout_constraintStart_toStartOf="@+id/heart1"
                app:layout_constraintTop_toBottomOf="@id/heart1" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="80dp"
                android:layout_height="20dp"
                android:gravity="center"
                android:text="Traditional"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="@+id/leaf2"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintStart_toStartOf="@+id/leaf2"
                app:layout_constraintTop_toBottomOf="@id/leaf2" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="60dp"
                android:layout_height="20dp"
                android:gravity="center"
                android:text="Rare"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="@+id/leaf3"
                app:layout_constraintStart_toStartOf="@+id/leaf3"
                app:layout_constraintTop_toBottomOf="@id/leaf3" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="60dp"
                android:layout_height="20dp"
                android:gravity="center"
                android:text="Common"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="@+id/leaf4"
                app:layout_constraintStart_toStartOf="@+id/leaf4"
                app:layout_constraintTop_toBottomOf="@id/leaf4" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:gravity="center"
                android:text="Mind-Altering"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="@+id/drug"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintStart_toStartOf="@+id/drug"
                app:layout_constraintTop_toBottomOf="@+id/drug" />

            <TextView
                android:id="@+id/poisonous"
                android:layout_width="90dp"
                android:layout_height="20dp"
                android:gravity="center"
                android:text="Toxic"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="@+id/poison"
                app:layout_constraintStart_toStartOf="@+id/poison"
                app:layout_constraintTop_toBottomOf="@id/leaf4" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="60dp"
                android:layout_height="20dp"
                android:gravity="center"
                android:text="Edible"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="@+id/edible"
                app:layout_constraintStart_toStartOf="@+id/edible"
                app:layout_constraintTop_toBottomOf="@+id/edible">


            </TextView>


        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>

